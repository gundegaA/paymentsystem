		this.name = name;
		this.bankAccount = bankAccount;
		this.grossWage = grossWage;
	}

	@Override
	public String name() {
		
		return null;
	}

	@Override
	public Double grossPayment() {
		
		return null;
	}

	@Override
	public Integer bankAccount() {
		
		return null;
	}

	
	
	
}
